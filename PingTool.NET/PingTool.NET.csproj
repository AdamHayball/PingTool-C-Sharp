<Project Sdk="Microsoft.NET.Sdk" >
		<PropertyGroup>
			<OutputType>WinExe</OutputType>
			<TargetFramework>net6.0-windows</TargetFramework>
			<RootNamespace>PingTool</RootNamespace>
			<AssemblyName>PingTool</AssemblyName>
			<Nullable>enable</Nullable>
			<UseWindowsForms>true</UseWindowsForms>
			<ImplicitUsings>enable</ImplicitUsings>
			<ApplicationIcon>Resources\SPTMulti.ico</ApplicationIcon>
			<Copyright>Copyright © Adam Hayball 2023</Copyright>
			<AssemblyVersion>1.3.0</AssemblyVersion>
			<AssemblyInformationalVersion>1.3.0-Beta</AssemblyInformationalVersion>
			<FileVersion>1.3.0</FileVersion>
		</PropertyGroup>
		<ItemGroup>
			<PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
			<PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
			<PackageReference Include="WinForms.DataVisualization" Version="1.8.0" />
		</ItemGroup>
		<ItemGroup>
			<Compile Update="Properties\Settings.Designer.cs">
				<DesignTimeSharedInput>True</DesignTimeSharedInput>
				<AutoGen>True</AutoGen>
				<DependentUpon>Settings.settings</DependentUpon>
			</Compile>
		</ItemGroup>
		<ItemGroup>
			<None Update="Properties\Settings.settings">
				<Generator>SettingsSingleFileGenerator</Generator>
				<LastGenOutput>Settings.Designer.cs</LastGenOutput>
			</None>
		</ItemGroup>
</Project>